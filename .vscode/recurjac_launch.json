{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": "true",
            "args": [
                "--task", "robustness",
                "--numimage", "10" ,
                "--targettype", "random",
